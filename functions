#include <iostream>
#include <cstdio>
using namespace std;


int max_of_four(int a, int b, int c, int d)
{
    if(a>b&&a>c&&a>d)
    {
        //printf(a);
        return a;
    }
    if(b>a&&b>c&&b>d)
    {
        //printf(b);
        return b;
    }
    if(c>a&&c>b&&c>d)
    {
       // printf(c);
       return c;
    }
   else
   {
        //printf(d);
        return d;
    }
}

int main() {
    int a, b, c, d;
    //scanf("%d %d %d %d", &a, &b, &c, &d);
    cin>>a>>b>>c>>d;
    int ans = max_of_four(a, b, c, d);
    //printf("%d", ans);
    cout<<ans;
    
    return 0;
}
